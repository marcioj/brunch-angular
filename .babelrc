{
  "presets": ["es2015"],
  "plugins": [
    "external-helpers",
    "transform-inline-environment-variables",
    ["angular-annotate", ["angular", "ui.router"]]
  ],
  "passPerPreset": true,
  "env": {
    "test": {
      "plugins": [[ "__coverage__", { "ignore": ["client/**/*.spec.js", "spec/*"] } ]]
    }
  }
}
